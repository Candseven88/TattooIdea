# ZhipuAI API Configuration
ZHIPUAI_API_KEY=your_api_key_here
NEXT_PUBLIC_API_ENABLED=true

# PayPal Configuration
# 在开发环境中使用沙盒模式，在生产环境中使用live模式
NEXT_PUBLIC_PAYPAL_MODE=sandbox
# 从PayPal开发者控制台获取的客户端ID
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_client_id_here
# 从PayPal开发者控制台获取的密钥
NEXT_PUBLIC_PAYPAL_SECRET=your_paypal_secret_here
# 支付货币代码
NEXT_PUBLIC_PAYPAL_CURRENCY=USD

# Analytics Configuration
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_google_analytics_id_here
NEXT_PUBLIC_MICROSOFT_CLARITY_ID=your_microsoft_clarity_id_here 
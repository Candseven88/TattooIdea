# TattooIdea AI 模板配置

# 选择AI提供商: zhipu, openai, stabilityai, custom
AI_PROVIDER=zhipu

# 智谱AI配置（基础免费模式）
ZHIPUAI_API_KEY=your_zhipu_api_key_here

# OpenAI配置
OPENAI_API_KEY=your_openai_api_key_here

# StabilityAI配置（高级付费模式）
STABILITYAI_API_KEY=your_stabilityai_api_key_here
# 高级模式需要同时配置OpenAI和StabilityAI的API密钥
# OpenAI用于文本分析，StabilityAI用于图像生成

# 自定义AI提供商配置
CUSTOM_API_KEY=your_custom_api_key_here
CUSTOM_API_URL=your_custom_api_url_here
CUSTOM_MODEL=your_custom_model_name_here

# 应用配置
NEXT_PUBLIC_API_ENABLED=true
NEXT_PUBLIC_PREMIUM_MODE_ENABLED=true 